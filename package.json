{
    "name": "ayp1-pseudocode",
    "displayName": "DIT - AyP1 Pseudocode",
    "description": "Una extensión sencilla para resaltado de pseudocódigo. DIT. Universidad Nacional de la Patagonia. Argentina.",
    "publisher": "dafinf",
    "icon": "imagenes/icon.png",
    "version": "1.5.2",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "pseudocode",
            "aliases": ["AyP 1 Pseudocode", "pseudocode"],
            "extensions": [".pseudo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pseudocode",
            "scopeName": "source.pseudo",
            "path": "./syntaxes/pseudocode.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "pseudocode",
                "path": "./snippets.json"
            }
        ]
    }
}